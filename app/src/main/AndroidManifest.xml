<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.white"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_FORMAT_FILESYSTEMS"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>


    <application
        android:name=".other.TravelApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.white.other.utils.GlideConfiguration"
            android:value="GlideModule"/>

        <activity android:name=".other.ui.activity.MainActivity">
        </activity>
        <activity android:name=".other.ui.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".my.ui.LoginActivity">

        </activity>
        <activity android:name=".other.ui.activity.GuideActivity">

        </activity>
        <activity android:name=".other.ui.activity.AdActivity">
        </activity>

        <activity android:name=".other.ui.activity.Des_City_Activity"/>

        <activity android:name=".my.ui.RegisterActivity"/>

        <activity android:name=".other.ui.activity.WebBuyActivity"/>

        <activity android:name=".other.ui.activity.WebNormalActivity"/>

        <activity android:name=".other.ui.activity.ProductActivity"/>

    </application>

</manifest>